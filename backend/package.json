{
  "name": "fleet-backend",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "build:full": "tsc && prisma generate",
    "start": "node dist/index.js",
    "prisma:generate": "dotenv -c development -- prisma generate",
    "prisma:migrate": "dotenv -c development -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -c development -- prisma migrate dev",
    "prisma:deploy": "dotenv -c development -- prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "postinstall": "dotenv -c development -- prisma generate || prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.6.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "dotenv-cli": "^7.4.4",
    "prisma": "^6.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.x"
  }
}
